{
  "name": "rest-express",
  "version": "1.0.0",
  "description": "Wedding Directory Platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify",
    "build:deploy": "npm run optimize-images && vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "type-check": "npx tsc --noEmit",
    "optimize-images": "node scripts/optimize-images.js",
    "audit:performance": "node scripts/performance-audit.js",
    "analyze": "ANALYZE=true npm run build",
    "redis:start": "redis-server --daemonize yes",
    "redis:stop": "redis-cli shutdown",
    "cache:clear": "redis-cli flushall"
  },
