{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify",
    "build:production": "vite build --config vite.config.production.ts && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify",
    "build:deploy": "npm run optimize-images && vite build --config vite.config.deploy.ts",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:bundle:full": "npm run build && npm run analyze:bundle",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:setup": "node scripts/setup-neon-db.js",
    "db:test-supabase": "tsx scripts/test-supabase.js",
    "db:status": "tsx scripts/test-supabase.js",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx scripts/seed-supabase.js",
    "db:migrate-postgres": "tsx scripts/migrate-to-postgres.ts",
    "db:setup:test": "echo 'Setting up test database'",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:check": "jest --coverage --coverageReporters=text-summary | grep 'Lines'",
    "test:coverage:trend": "node scripts/test-monitoring.js timing",
    "test:integration": "jest --testPathPattern=integration",
    "test:api": "jest --testPathPattern=api",
    "test:performance": "jest --testPathPattern=performance",
    "test:timing-report": "node scripts/test-monitoring.js timing",
    "test:flaky-detection": "node scripts/test-monitoring.js flaky",
    "test:health-report": "node scripts/test-monitoring.js report",
    "test:snapshots:validate": "jest --updateSnapshot --ci",
    "test:snapshots:freshness": "find . -name '*.snap' -mtime +30 -ls",
    "test:smoke:staging": "node scripts/test-monitoring.js health-check staging",
    "test:smoke:production": "node scripts/test-monitoring.js health-check production",
    "health-check:local": "node scripts/test-monitoring.js health-check local",
    "health-check:staging": "node scripts/test-monitoring.js health-check staging",
    "health-check:production": "node scripts/test-monitoring.js health-check production",
    "health-check:database": "node scripts/test-monitoring.js health-check database",
    "monitor:performance": "node scripts/test-monitoring.js performance",
    "monitor:web-vitals": "node scripts/test-monitoring.js web-vitals",
    "analyze:bundle-size": "npm run build:analyze && node scripts/analyze-bundle.js",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "deploy:staging": "npm run build:staging && npm run migrate:up && npm run health:check",
    "deploy:production": "npm run build:production && npm run migrate:up && npm run health:check && npm run audit:performance",
    "build:staging": "cross-env NODE_ENV=staging vite build --config vite.config.staging.ts",
    "migrate:up": "node scripts/deployment/migrate.js up",
    "migrate:status": "node scripts/deployment/migrate.js status",
    "migrate:rollback": "node scripts/deployment/migrate.js rollback",
    "rollback:list": "node scripts/deployment/rollback.js list",
    "rollback:to": "node scripts/deployment/rollback.js rollback",
    "rollback:record": "node scripts/deployment/rollback.js record",
    "health:check": "curl -f http://localhost:5002/health || exit 1",
    "health:detailed": "curl -s http://localhost:5002/health | jq",
    "backup:create": "node scripts/deployment/backup.js create",
    "backup:list": "node scripts/deployment/backup.js list",
    "backup:restore": "node scripts/deployment/backup.js restore",
    "backup:cleanup": "node scripts/deployment/backup.js cleanup",
    "optimize:production": "node scripts/optimize-production.js",
    "optimize-images": "node scripts/optimize-images.js",
    "audit:performance": "node scripts/performance-audit.js",
    "verify:deployment": "node scripts/verify-deployment.js",
    "test:production": "node scripts/production-tests.js",
    "test:e2e": "echo 'End-to-end tests - implement with Playwright or Cypress'",
    "backup:database": "echo 'Database backup - implement based on your DB'",
    "restore:database": "echo 'Database restore - implement based on your DB'",
    "logs:production": "echo 'View production logs - implement based on your hosting'",
    "maintenance:on": "echo 'Enable maintenance mode'",
    "maintenance:off": "echo 'Disable maintenance mode'"
  },
